<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="WinGame">
   <children>
      <Label text="You win!">
         <font>
            <Font size="48.0" />
         </font>
      </Label>
      <Label text="Congrats, how does it&#10;feel to win?">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="100.0">
         <children>
            <Button onAction="#onTryAgainPressed" mnemonicParsing="false" text="Try Again">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button onAction="#onQuitPressed" mnemonicParsing="false" text="Quit">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
