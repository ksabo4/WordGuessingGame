<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="50.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoseGame">
   <children>
      <Label text="Game Over">
         <font>
            <Font size="64.0" />
         </font>
      </Label>
      <Button onAction="#onTryAgainPressed"  mnemonicParsing="false" text="Try again">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button onAction="#onQuitPressed" mnemonicParsing="false" text="Quit">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
   </children>
</VBox>
